  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ionic build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "fcm": "concat www/ngsw-worker.js www/firebase-messaging-sw.js -o www/combined-worker.js",
    "deploy": "firebase deploy --only hosting",
    "serve": "ionic serve",
    "lab": "ionic serve -l",
    "pwa": "ionic build --prod && npm run fcm",
    "ios": "ionic cordova emulate ios -lsc --target=\"iPhone-X\" -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "ios-prod": "ionic cordova emulate ios --prod --target=\"iPhone-X\" -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "android": "ionic cordova run android -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "android-prod": "ionic cordova run android --prod -- --buildFlag=\"-UseModernBuildSystem=0\""
  },
